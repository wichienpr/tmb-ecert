{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "actions/*": ["src/app/buckwaframework/common/actions/*"],
      "reducers/*": ["src/app/buckwaframework/common/reducers/*"],
      "stores/*": ["src/app/buckwaframework/common/stores/*"],
      "commons/*": ["src/app/buckwaframework/common/*"],
      "projects/*": ["src/app/buckwaframework/project/*"],
      "components/*": ["src/app/buckwaframework/common/components/*"],
      "configs/*": ["src/app/buckwaframework/common/configs/*"],
      "models/*": ["src/app/buckwaframework/common/models/*"],
      "helpers/*": ["src/app/buckwaframework/common/helpers/*"],
      "services/*": ["src/app/buckwaframework/common/services/*"]
    }
  }
}
